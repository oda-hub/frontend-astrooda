<?php

/**
 * Implements hook_theme().
 */
function mmoda_spi_acs_theme($existing, $type, $theme, $path)
{
  return [
  'mmoda_spi_acs_form'=> [
    'render element' => 'form',
  ]
];

}

function mmoda_spi_acs_install($is_syncing)
{
