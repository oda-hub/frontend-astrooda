<?php

/**
 * Implements hook_theme().
 */
function mmoda_magic_theme($existing, $type, $theme, $path)
{
  return [
  'mmoda_magic_form'=> [
    'render element' => 'form',
  ]
];

}

function mmoda_magic_install($is_syncing)
{
  mmoda_help_pages_book_publish('mmoda_magic');
}

function mmoda_magic_uninstall($is_syncing)
{
  mmoda_help_pages_book_unpublish('mmoda_magic');
}
